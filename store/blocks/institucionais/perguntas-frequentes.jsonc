    }
  },
  "rich-text#perguntas-frequentes-2-1": {
    "props": {
      "text": "After logging into your account, you can find this info at the link Orders."
    }
  },


  "disclosure-layout#perguntas-frequentes-3": {
    "children": [
      "disclosure-trigger#perguntas-frequentes-3",
      "disclosure-content#perguntas-frequentes-3"
    ]
  },
  "disclosure-trigger#perguntas-frequentes-3": {
    "props": {
      "blockClass": "question"
    },
    "children": [
      "rich-text#perguntas-frequentes-3",
